<?xml version="1.0"?>
<launch>

	<group>
		<machine name="pc2" address="192.168.0.102" ros-root="$(env ROS_ROOT)" ros-package-path="$(env ROS_PACKAGE_PATH)" default="true"/>

		  <!-- The camera node -->
		  <node ns="left" name="color_camera" pkg="prosilica_camera" type="prosilica_node" output="screen">
		    <param name="ip_address" type="str" value="169.254.236.139"/>
		    <param name="trigger_mode" type="str" value="streaming"/>
		    <remap from="camera/image_raw" to="image_raw"/>
		    <remap from="camera/camera_info" to="camera_info" />
		    <remap from="color_camera/parameter_descriptions" to="parameter_descriptions" />
		    <remap from="color_camera/parameter_updates" to="parameter_updates" />
		    <rosparam command="load" file="$(find uh_simple_scenario1)/launch/camera_setting.yaml" />
		  </node>

		  <!-- The camera node -->
		  <node ns="right" name="color_camera" pkg="prosilica_camera" type="prosilica_node" output="screen">
		    <param name="ip_address" type="str" value="169.254.47.10"/>
		    <param name="trigger_mode" type="str" value="streaming"/>
		    <remap from="camera/image_raw" to="image_raw"/>
		    <remap from="camera/camera_info" to="camera_info" />
		    <remap from="color_camera/parameter_descriptions" to="parameter_descriptions" />
		    <remap from="color_camera/parameter_updates" to="parameter_updates" />
		    <rosparam command="load" file="$(find uh_simple_scenario1)/launch/camera_setting.yaml" />
		  </node>

		  <node ns="left" pkg="image_proc" type="image_proc" respawn="false" name="image_proc" />
		  <node ns="right" pkg="image_proc" type="image_proc" respawn="false" name="image_proc" />

		  <!--node ns="stereo" pkg="stereo_image_proc" type="stereo_image_proc" respawn="false" name="stereo_image_proc">
		    <remap from="stereo_image_proc/parameter_descriptions" to="parameter_descriptions" />
		    <remap from="stereo_image_proc/parameter_updates" to="parameter_updates" />
		  </node-->


		  <!--node pkg="image_view" type="image_view" respawn="false" name="viewer_color_left">
		    <param name="autosize" type="bool" value="true" />
		    <param name="window_name" type="str" value="Prosilica Camera (left)" />
		    <remap from="image" to="left/image_raw" />
		  </node-->
	</group>

</launch>
